config_version=5

[application]

config/name="moss grotto"
run/main_scene="res://main.tscn"
config/features=PackedStringArray("4.5", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

SeasonManager="*res://components/seasons/season_manager.gd"
WeatherManager="*res://components/weather/weather manager.gd"
TimeOfDayManager="*res://components/time/time of day manager.gd"

[display]

window/size/viewport_width=1360
window/size/viewport_height=600
window/stretch/scale=2.0

[editor]

movie_writer/video_quality=1.0
movie_writer/movie_file="C:/Godot/projects/pong-perhaps/clips/clip.avi"

[editor_plugins]

enabled=PackedStringArray("res://addons/parallax2d_preview/plugin.cfg")

[file_customization]

